	}

	public int doSomething() {
		return 1;
	}
	
	public void hello(){
		//hello
		int i;
	}

	public static void main (String[] args){
		boolean[] bArray = new boolean[5];
		int[] iArray = new int[5];
		double[] dArray = new double[5];
		Object[] oArray = new Object [5];
		Boolean[] BArray = new Boolean[5];
		String[] SArray = new String[5];
		System.out.println(bArray[1]);
		System.out.println(iArray[1]);
		System.out.println(oArray[1]);
		System.out.println(BArray[0]);
		System.out.println(bArray[3]);
		System.out.println(SArray[2]);
		System.out.println(dArray[4]);

		Random rand = new Random();
		boolean [] bools2 = {
			rand.nextBoolean(),
			rand.nextBoolean(),
			rand.nextBoolean(),
			rand.nextBoolean()
		};
		System.out.println(Arrays.toString(bools2));

		int [] int2 = {
			rand.nextInt(),
			rand.nextInt(),
			rand.nextInt(),
			rand.nextInt()
		};
		System.out.println(Arrays.toString(int2));
		
		double [] double2 = {
			rand.nextDouble(),
			rand.nextDouble(),
			rand.nextDouble(),
			rand.nextDouble()
		};
		System.out.println(Arrays.toString(double2));

		}
	}

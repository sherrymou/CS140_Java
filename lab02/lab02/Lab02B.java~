package lab02;

import java.awt.Rectangle;

public class Lab02B {

	private Rectangle rect

	public int area()

	public Lab02B(int[] numbers) {
		//Constructor
		if (length(numbers)>=4) {
			Rectangle rect = new Rectangle(x=numbers[0],
				y=numbers[1],width=numbers[2], height=numbers[3]);
		}else{
			this.Rectangle(0,0,0,0);
		}
		
	}	




}

